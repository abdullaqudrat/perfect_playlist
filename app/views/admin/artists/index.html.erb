<div class="sub_header">
  <h1>Admin: All Artists</h1>
</div>

<h3><%= link_to "Create a New Artist", new_artist_path %></h3>

<% @artists.each do |artist| %>
  <div id="artist-<%=artist.id%>">
    <p><%= link_to artist.name, artist_path(artist) %></p>
    <p><%= link_to 'delete', admin_artist_path(artist), method: :delete, data: {confirm: "Really delete #{artist}?"} %></p>
  </div>
<% end %>
