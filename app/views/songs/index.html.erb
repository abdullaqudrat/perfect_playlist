<div class="sub_header">
  <h1>All Songs</h1>
</div>

<h3><%= link_to "Create a New Song", new_song_path %></h3>

<% @songs.each do |song| %>
  <div id="song-<%=song.id%>">
    <p><%= link_to song.title, song_path(song) %></p>
    <p>Artist: <%= link_to song.artist.name, artist_path(song.artist) %></p>
  </div>
<% end %>
